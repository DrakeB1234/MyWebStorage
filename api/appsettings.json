{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FileUploadPath": "C:/Photos",
  "MaxFileUploadSize": 31457280,
  "ImageCacheTimeDays": 7,
  "AllowedExtensions": [
    ".jpg", ".png", ".jpeg", ".gif", ".mp4", ".avi", ".mov", ".webp", ".webm"
  ],
  "ImageExtensions": [
    ".jpg", ".png", ".jpeg", ".webp", ".gif", ".svg"
  ],
  "VideoExtensions": [
    ".mp4", ".mov", ".avi", ".webm"
  ],
  "DocumentExtensions": [
    ".txt", ".docx", ".doc", ".pdf"
  ],
  "JwtSettings": {
    "Issuer": "MWS-MyWebStorage",
    "Audience": "MWS-MyWebStorage-User",
    "ExpirationInMinutes": 60
  }
}
